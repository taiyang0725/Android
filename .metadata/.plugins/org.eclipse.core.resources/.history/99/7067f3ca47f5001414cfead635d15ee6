package com.anxiong.study;
/**
 * 反射
 * */
public class Reflect {

	
	public static void main(String[] args) {
		/**
		 * 方法一
		 * */
		
		A a=new A();
		
		
		Class<?> a1=a.getClass();
		
		System.out.println("方法一"+a1);
		/**
		 * 方法二
		 * */
		
		Class<String> class1=String.class;
		System.out.println("方法二"+class1);
		
		/**
		 * 方法三
		 * */
		
		Class<?> class2=null;
		System.out.println("方法二"+class11);

	}
}

class A {
}