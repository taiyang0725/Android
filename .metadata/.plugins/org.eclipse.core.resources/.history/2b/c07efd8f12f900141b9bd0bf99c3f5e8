package com.anxiong.picture;

import com.example.testviewpage.R;

public class Picture {
	
	public static Integer[] getPic() {
		Integer[] bg_normal = { R.drawable.alc, R.drawable.ala, R.drawable.ale,
				R.drawable.alg };
		Integer[] bg_focus = { R.drawable.alb, R.drawable.al_, R.drawable.ald,
				R.drawable.alf };
		return bg_focus;
        
	}

}
